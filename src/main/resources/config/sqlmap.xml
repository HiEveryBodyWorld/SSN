        <typeAlias type="com.news.entity.OthersManage" alias="OthersManage"/>
        <typeAlias type="com.news.entity.ManageImages" alias="ManageImages"/>
    </typeAliases>
    <mappers>
        <mapper resource="mappers/admin.xml" />
        <mapper resource="mappers/adminRole.xml" />
        <mapper resource="mappers/resources.xml" />
        <mapper resource="mappers/role.xml" />
        <mapper resource="mappers/roleResources.xml" />
        <mapper resource="mappers/images.xml" />

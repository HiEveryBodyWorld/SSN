<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <typeAlias type="com.news.entity.Admin" alias="Admin"/>
        <typeAlias type="com.news.entity.AdminRole" alias="AdminRole"/>
        <typeAlias type="com.news.entity.Resources" alias="Resources"/>
        <typeAlias type="com.news.entity.Role" alias="Role"/>
        <typeAlias type="com.news.entity.RoleResources" alias="RoleResources"/>
        <typeAlias type="com.news.entity.Images" alias="Images"/>

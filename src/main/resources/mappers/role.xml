<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="sqlmap.Role">
	<!-- 全部列 -->
	<resultMap id="BaseResultMap" type="Role" >
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="isDel" property="isDel" jdbcType="INTEGER" />
		<result column="creatorId" property="creatorId" jdbcType="INTEGER" />
	    <result column="creator" property="creator" jdbcType="VARCHAR" />
	    <result column="createTime" property="createTime" jdbcType="TIMESTAMP" />
	    <result column="operatorId" property="operatorId" jdbcType="INTEGER" />
	    <result column="operator" property="operator" jdbcType="VARCHAR" />
	    <result column="operateTime" property="operateTime" jdbcType="TIMESTAMP" />
	    <result column="name" property="name" jdbcType="VARCHAR" />
	    <result column="description" property="description" jdbcType="VARCHAR" />
	</resultMap>
	<!-- 部分列 -->
	<resultMap id="BaseResultMapType0" type="Role" >
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="name" property="name" jdbcType="VARCHAR" />
	    <result column="description" property="description" jdbcType="VARCHAR" />
	</resultMap>
	<!--查询全部列  -->
	<sql id="Base_Column_List">
		id,
		isDel,
		creatorId,
		creator,
		createTime,
		operatorId,
		operator,
		operateTime,
		name,
		description
	</sql>
	<!--查询列参数  -->
	<sql id="queryMap">
		<if test="id != null">
			and id = #{id,jdbcType=INTEGER}
		</if>
		<if test="isDel != null">
			and isDel = #{isDel,jdbcType=INTEGER}
		</if>
		<if test="creatorId != null">
			and creatorId = #{creatorId,jdbcType=INTEGER}
		</if>
		<if test="creator != null">
			and creator = #{creator,jdbcType=VARCHAR}
		</if>
		<if test="createTimeStart !=null and createTimeStart != ''">
    		<![CDATA[and DATE_FORMAT(createTime, '%Y-%m-%d') >=  DATE_FORMAT(#{createTimeStart}, '%Y-%m-%d')]]>
    	</if>
    	<if test="createTimeEnd !=null and createTimeEnd != ''">
    		<![CDATA[and DATE_FORMAT(createTime, '%Y-%m-%d') <=  DATE_FORMAT(#{createTimeStart}, '%Y-%m-%d')]]>
    	</if>
    	<if test="operatorId != null">
			and operatorId = #{operatorId,jdbcType=INTEGER}
		</if>
		<if test="operator != null">
			and operator = #{operator,jdbcType=INTEGER}
		</if>
		<if test="operateTimeStart !=null and operateTimeStart != ''">
    		<![CDATA[and DATE_FORMAT(operateTime, '%Y-%m-%d') >=  DATE_FORMAT(#{operateTimeStart}, '%Y-%m-%d')]]>
    	</if>
    	<if test="operateTimeEnd !=null and operateTimeEnd != ''">
    		<![CDATA[and DATE_FORMAT(operateTime, '%Y-%m-%d') <=  DATE_FORMAT(#{operateTimeEnd}, '%Y-%m-%d')]]>
    	</if>
    	<if test="name != null">
			and name like concat('%', #{name,jdbcType=VARCHAR}, '%')
		</if>
		<if test="description != null">
			and description = #{description,jdbcType=VARCHAR}
		</if>
    </sql>
    <!--更新列参数  -->
    <sql id="updateMap">
    	<if test="isDel != null">
			isDel = #{isDel,jdbcType=INTEGER},
		</if>
		<if test="operatorId != null">
			operatorId = #{operatorId,jdbcType=INTEGER},
		</if>
		<if test="operator != null">
			operator = #{operator,jdbcType=VARCHAR},
		</if>
		<if test="operateTime != null">
			operateTime = #{operateTime,jdbcType=TIMESTAMP},
		</if>
    	<if test="name != null">
			name = #{name,jdbcType=VARCHAR},
		</if>
		<if test="description != null">
			description = #{description,jdbcType=VARCHAR},
		</if>
    </sql>
    <!-- 总记录数 -->
    <select id="findListCount" parameterType="map" resultType="int">
		select count(1)
		from tb_role
		where 1 = 1
		<include refid="queryMap"/>
		order by id desc
	</select>
	<!-- 查询列表 -->
	<select id="findList" parameterType="map" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from tb_role
		where  1 = 1
		<include refid="queryMap"/>
		order by id desc
		limit   #{startIndex,jdbcType=INTEGER},#{rows,jdbcType=INTEGER}
	</select>
	<!-- 查询所有 -->
	<select id="findListAll" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from tb_role
		where 1 = 1
		order by id asc
	</select>
	<!-- 查询所有 -->
	<select id="findListAllWithMap" parameterType="map"  resultMap="BaseResultMap">
		select
	    <include refid="Base_Column_List" />
		from tb_role
		where 1=1
		<include refid="queryMap"/>
		order by id asc
	</select>
	<!--查询一个  -->
	<select id="findById" parameterType="java.lang.Integer" resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from tb_role
		where id = #{id,jdbcType=INTEGER}
	</select>
	<!-- 插入 -->
	<insert id="insert" parameterType="Role"  useGeneratedKeys="true" keyProperty="id">
		insert into tb_role (<include refid="Base_Column_List" />) values 
      	(
	      #{id,jdbcType=INTEGER}, 
	      #{isDel,jdbcType=INTEGER}, 
	      #{creatorId,jdbcType=INTEGER}, 
	      #{creator,jdbcType=VARCHAR}, 
	      #{createTime,jdbcType=TIMESTAMP},
	      #{operatorId,jdbcType=INTEGER},
	      #{operator,jdbcType=VARCHAR},
	      #{operateTime,jdbcType=TIMESTAMP},
	      #{name,jdbcType=VARCHAR},
	      #{description,jdbcType=VARCHAR}
      	)
	</insert>
	<!-- 更新-->
	<update id="update" parameterType="Role">
		update tb_role
	    <set>
	      <include refid="updateMap"/>
	    </set>
	    	where id = #{id,jdbcType=INTEGER}
	</update>
	<!-- 删除 -->
	<delete id="delete" parameterType="java.lang.Integer">
		delete from tb_role
		where id = #{id,jdbcType=INTEGER}
	</delete>
	<!-- 账号密码精确匹配 -->
	<select id="existsEntity" parameterType="map"  resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from tb_role
		where  1 = 1
		<if test="name != null">
			and name = #{name,jdbcType=VARCHAR}
		</if>
		<if test="description != null">
			and description = #{description,jdbcType=VARCHAR}
		</if>
		order by id desc
	</select>
	
</mapper>